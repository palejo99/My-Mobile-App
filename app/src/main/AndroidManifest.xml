<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alejop.crudpersona">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".globalstate.GlobalState"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DashBoard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IngresoPersona" />
        <activity android:name=".globalstate.ListaPersona" />
        <activity android:name=".globalstate.BuscarPersona" />
        <activity android:name=".Empresario.Consignar" />
        <activity android:name=".Empleado.InterfazUsuario" />
        <activity android:name=".Administrador.EliminarUsuario"></activity>
    </application>

</manifest>